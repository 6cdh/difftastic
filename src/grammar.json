{
  "name": "ruby",
  "rules": {
    "program": {
      "type": "SYMBOL",
      "name": "_compound_statement"
    },
    "_compound_statement": {
      "type": "REPEAT",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_statement"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_terminator"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_expression"
        }
      ]
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_argument"
        }
      ]
    },
    "_argument": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_primary"
        }
      ]
    },
    "_primary": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "("
            },
            {
              "type": "SYMBOL",
              "name": "_compound_statement"
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_variable"
        }
      ]
    },
    "_variable": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "nil"
        },
        {
          "type": "STRING",
          "value": "self"
        }
      ]
    },
    "identifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "@"
              },
              {
                "type": "STRING",
                "value": "$"
              }
            ]
          }
        },
        {
          "type": "PATTERN",
          "value": "[a-zA-Z_][a-zA-Z0-9_]*"
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "#"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "=begin\n"
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "PATTERN",
                  "value": ".*\\n"
                }
              },
              {
                "type": "STRING",
                "value": "=end\n"
              }
            ]
          }
        ]
      }
    },
    "_line_break": {
      "type": "STRING",
      "value": "\n"
    },
    "_terminator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_line_break"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "SYMBOL",
      "name": "_line_break"
    },
    {
      "type": "PATTERN",
      "value": "[ \\t\\r]"
    }
  ],
  "conflicts": []
}