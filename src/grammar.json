{
  "name": "c_sharp",
  "rules": {
    "compilation_unit": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "using_directive"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "namespace_declaration"
          }
        }
      ]
    },
    "using_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "using"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "static"
                },
                {
                  "type": "SYMBOL",
                  "name": "name_equals"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "qualified_name"
            },
            {
              "type": "SYMBOL",
              "name": "identifier_name"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "namespace_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "namespace"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "qualified_name"
            },
            {
              "type": "SYMBOL",
              "name": "identifier_name"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "namespace_declaration"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "qualified_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "identifier_name"
            },
            {
              "type": "SYMBOL",
              "name": "qualified_name"
            },
            {
              "type": "SYMBOL",
              "name": "alias_qualified_name"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "SYMBOL",
          "name": "identifier_name"
        }
      ]
    },
    "alias_qualified_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "global"
        },
        {
          "type": "STRING",
          "value": "::"
        },
        {
          "type": "SYMBOL",
          "name": "identifier_name"
        }
      ]
    },
    "name_equals": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier_name"
        },
        {
          "type": "STRING",
          "value": "="
        }
      ]
    },
    "global": {
      "type": "STRING",
      "value": "global"
    },
    "static": {
      "type": "STRING",
      "value": "static"
    },
    "identifier_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z-0-9]*"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "//"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/*"
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "PATTERN",
                      "value": "[^*]"
                    },
                    {
                      "type": "PATTERN",
                      "value": "\\*[^\\/]"
                    }
                  ]
                }
              },
              {
                "type": "STRING",
                "value": "*/"
              }
            ]
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s+"
    }
  ],
  "conflicts": []
}