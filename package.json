{
  "name": "tree-sitter-elm",
  "version": "1.1.1",
  "description": "Tree sitter definitions for elm",
  "main": "index.js",
  "keywords": [
    "parser",
    "lexer"
  ],
  "author": "Razze",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.12.1",
    "prebuild-install": "^5.2.4"
  },
  "devDependencies": {
    "prebuild": "^8.2.0",
    "tree-sitter-cli": "^0.14.5"
  },
  "scripts": {
    "build": "tree-sitter generate && node-gyp build --debug",
    "install": "prebuild-install || node-gyp rebuild",
    "prebuild": "prebuild -r electron -t 1.6.0 -t 1.7.0 -t 1.8.0 -t 2.0.0 -t 3.0.0 -t 4.0.0 --strip --verbose",
    "prebuild:upload": "prebuild --upload-all",
    "parse-basic": "tree-sitter parse ./examples/Basic.elm",
    "parse-test": "tree-sitter parse ./examples/test.elm",
    "test": "tree-sitter test && script/parse-examples",
    "test-windows": "tree-sitter test",
    "tests-only": "tree-sitter test"
  },
  "repository": "https://github.com/razzeee/tree-sitter-elm"
}
